OPENWEATHER_API_KEY=your_openweather_api_key_here

# AI Service - Get your key from https://makersuite.google.com/app/apikey  
GEMINI_API_KEY=your_gemini_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development